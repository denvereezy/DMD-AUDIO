<div class="knobs row"><br><br>
    <div id="time-knob" class="col-sm-4">
        <input id="time" class="duration_dial" value="0" data-width="230" data-height="230" data-bgColor="#34495E" data-fgcolor="#0095D8" data-thickness="0.30" data-min="0" data-max="1000" data-displayInput="true" data-displayPrevious="true">
        </br>
        <div class="btn-group">
            <button id="repeat" class="btn btn-default btn-lg btn-cmd btn-toggle" type="button" title="Repeat" data-cmd="repeat"><i class="fa fa-repeat"></i></button>
            <button id="random" class="btn btn-default btn-lg btn-cmd btn-toggle" type="button" title="Random" data-cmd="random"><i class="fa fa-random"></i></button>
            <button id="single" class="btn btn-default btn-lg btn-cmd btn-toggle" type="button" title="Single" data-cmd="single"><i class="fa fa-refresh"></i></button>
        </div>
    </div>
    <div id="playing" class="col-sm-4">
        <marquee>
            <div id="nowPlay">
                <span class="left" id="npAction"></span>
                <span class="right" id="npTitle"></span>
            </div>
        </marquee>
        <div id="audiowrap">
            <div id="audio0">
                <audio preload id="audio1" hidden></audio>
            </div>
        </div>
        </br>
        <!-- <div id="plwrap">
                    <ul id="plList"></ul>
                </div> -->
        <!-- </div> -->
        <!-- </div> -->
        <div class="eq">
            <div class="controls">
                <label>Low Gain</label>
                <input type="range" value="100" step="1" min="0" max="100" oninput="changeGain(this.value, 'lowGain');"></input>
            </div>
            <div class="controls">
                <label>Mid Gain</label>
                <input type="range" value="100" step="1" min="0" max="100" oninput="changeGain(this.value, 'midGain');"></input>
            </div>
            <div class="controls">
                <label>High Gain</label>
                <input type="range" value="100" step="1" min="0" max="100" oninput="changeGain(this.value, 'highGain');"></input>
            </div>
        </div>
        <canvas id='canvas' width="300" height="300"></canvas>
    </div>
    <div id="volume-knob" class="col-sm-4">
        <input id="volume" class="vol_dial" value="100" data-width="230" data-height="230" data-bgColor="#0095D8" data-thickness="0.25" data-skin="tron" data-cursor="true" data-angleArc="250" data-angleOffset="-125">
        </br>
        <div class="btn-group">
            <button id="volume_down" class="btn btn-default btn-lg btn-cmd btn-volume" type="button"><i class="fa fa-volume-down"></i></button>
            <button id="volume_off" class="btn btn-default btn-lg btn-cmd btn-volume" type="button"><i class="fa fa-volume-off"></i></button>
            <button id="volume_up" class="btn btn-default btn-lg btn-cmd btn-volume" type="button"><i class="fa fa-volume-up"></i></button>
        </div>
    </div>
</div>
